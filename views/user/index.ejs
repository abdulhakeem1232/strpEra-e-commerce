       #bannerCarousel {
    position: relative;
}

#bannerCarousel img {
    width: 100%;
	height: auto;
}

.carousel-inner {
    display: flex;
    align-items: center;
}

.carousel-item {
    position: relative;
    text-align: center;
}

.carousel-caption {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -60%);
    color: white;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.7);
    width: 100%; 
    box-sizing: border-box; 
}

.carousel-caption h5 {
    position: absolute;
	top: -30%;
    left: 80%;
    transform: translate(-50%, -50%);
    font-size: 2em;
    margin-bottom: 10px;
    color: white;
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.7);
    width: 100%;
    box-sizing: border-box;
}

.carousel-caption p {
    font-size: 1.4em;
    max-width: 100%; 
    margin: 0 auto; 
	 position: absolute;
	 top: 20%;
    left: 40%;
}

.carousel-item img {
    width: 100%; 
    height: auto;
}

    </style>
