	<!--================Login Box Area =================-->
	
				
				<section class="login_box_area section_gap">
					<div class="container">
						<div class="row">

							<!-- <div class="col-lg-6"> -->
								<!-- Signup Box Image goes here -->
							<!-- </div> -->
							<div class="col-12">
								<div class="login_form_inner">
									<h3>Create New Account</h3>
									<form class="row login_form" action="/regpost" method="post" id="signupForm" novalidate="novalidate">
										<div class="col-md-6 form-group">
											<input type="text" class="form-control" id="fname" name="fname" placeholder="First Name" required>
											<% if(expressFlash.nameeroor){%>
												<div class="alter text-danger text-start"><%= expressFlash.nameerror%></div>
								
											<%}%>
										</div>
										<div class="col-md-6 form-group">
											<input type="text" class="form-control" id="lname" name="lname" placeholder="Last Name" >
										</div>
										<div class="col-md-12 form-group">
											<input type="email" class="form-control" id="email" name="email" placeholder="Email" required>
											<% if(expressFlash.emailerror){%>
												<div class="alter text-danger text-start"><%= expressFlash.emailerror%></div>
								
											<%}%>
										</div>
										<div class="col-md-12 form-group">
											<input type="tel" class="form-control" id="phone" name="phone" placeholder="Phone Number" required>
											<% if(expressFlash.phoneerror){%>
												<div class="alter text-danger text-start"><%= expressFlash.phoneerror%></div>
								
											<%}%>
										</div>
										<div class="col-md-12 form-group">
											<input type="password" class="form-control" id="password" name="password" placeholder="Password" required>
											<% if(expressFlash.passworderror){%>
												<div class="alter text-danger text-start"><%= expressFlash.passworderror%></div>
								
											<%}%>
										</div>
										<div class="col-md-12 form-group">
											<input type="password" class="form-control" id="confirm_password" name="confirm_password" placeholder="Confirm Password" required>
											<% if(expressFlash.cpassworderror){%>
												<div class="alter text-danger text-start"><%= expressFlash.cpassworderror%></div>
								
											<%}%>
										</div>
										<div class="col-md-12 form-group">
											<button type="submit" class="primary-btn">Sign Up</button>
										</div>
										<div class="col-md-12">
											<p>Already have an account? <a href="/login">Login here</a></p>
										</div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</section>
				
